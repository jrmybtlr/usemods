{
  "name": "usemods-nuxt",
  "version": "1.15.0",
  "description": "Zippy little modifiers and utilities for your Nuxt app.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LittleFoxCompany/usemods.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm run dev:prepare && pnpm run test && pnpm run prepack && pnpm publish --no-git-checks",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.3",
    "usemods": "1.15.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/eslint-config": "^0.7.6",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "^22.17.0",
    "changelogen": "^0.5.7",
    "eslint": "^9.32.0",
    "nuxt": "^4.0.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}